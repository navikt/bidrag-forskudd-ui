{
  "name": "bidrag-behandling-ui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint ./src/**/*.{js,ts,tsx} package.json --quiet",
    "fix-lint": "eslint ./src/**/*.{js,ts,tsx} package.json --quiet --fix",
    "test": "cross-env NODE_ENV=TEST mocha --config .mocharc.js --spec ./src/__tests__/**/**/*.spec.{js,ts,tsx} --exit",
    "start": "",
    "dev": "cross-env-shell NODE_ENV=development webpack serve --config webpack.dev.config.js",
    "dev:mock": "cross-env ENABLE_MOCK=true NODE_ENV=development webpack serve --config webpack.dev.config.js",
    "build": "npm-run-all build:api build:webpack",
    "build:dev": "yarn build:api:dev && yarn build:webpack",
    "build:webpack": "webpack --config webpack.prod.config.js",
    "build:api": "npm-run-all -s api:person api:bidragGrunnlag api:bidragVedtak api:bidragBehandling api:bidragSak",
    "build:api:dev": "env-cmd -f env/.env.dev yarn build:api",
    "api:person": "swagger-typescript-api -p $BIDRAG_PERSON_URL/v3/api-docs --axios -n PersonApi.ts -o src/api",
    "api:bidragGrunnlag": "swagger-typescript-api -p $BIDRAG_GRUNNLAG_URL/v3/api-docs --axios -n BidragGrunnlagApi.ts -o src/api",
    "api:bidragVedtak": "swagger-typescript-api -p $BIDRAG_VEDTAK_URL/v3/api-docs --extract-enums --axios -n BidragVedtakApi.ts -o src/api",
    "api:bidragBehandling": "swagger-typescript-api -p $BIDRAG_BEHANDLING_URL/v3/api-docs --extract-enums --axios -n BidragBehandlingApi.ts -o src/api",
    "api:bidragSak": "swagger-typescript-api -p $BIDRAG_SAK_URL/v3/api-docs --axios -n BidragSakApi.ts -o src/api",
    "local:api:bidragBehandling": "swagger-typescript-api -p http://localhost:8999/v3/api-docs --extract-enums --axios -n BidragBehandlingApi.ts -o src/api"
  },
  "dependencies": {
    "@navikt/aksel-icons": "3.2.2",
    "@navikt/bidrag-ui-common": "^1.7.0",
    "@navikt/ds-css": "4.1.4",
    "@navikt/ds-css-internal": "3.4.3",
    "@navikt/ds-react": "4.1.4",
    "@navikt/ds-react-internal": "3.4.3",
    "@navikt/ds-tailwind": "4.1.4",
    "@tanstack/react-query": "^4.29.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/lodash": "^4.14.192",
    "axios": "^1.3.2",
    "echarts": "^5.4.1",
    "express": "^4.18.2",
    "lodash.debounce": "^4.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.2",
    "react-hook-form": "^7.43.2",
    "react-router-dom": "^6.7.0"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@swc/core": "^1.3.27",
    "@swc/register": "^0.1.10",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/mocha": "^10.0.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "autoprefixer": "^10.4.13",
    "chai": "^4.3.7",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.27.2",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.5",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.32.0",
    "eslint-config-eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "esm": "^3.2.25",
    "file-loader": "^6.2.0",
    "global-jsdom": "^8.7.0",
    "graphql": "^16.6.0",
    "html-webpack-plugin": "^5.5.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^21.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "mocha": "^10.2.0",
    "msw": "^0.49.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.3",
    "process": "^0.11.10",
    "react-refresh": "^0.14.0",
    "sinon": "^15.0.1",
    "swagger-typescript-api": "^12.0.2",
    "swc-loader": "^0.2.3",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.8.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}